{
  "expo": {
    "name": "PackMindPlus",
    "slug": "packmindplus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "entryPoint": "./index.ts",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#a6c13c",
      "androidMode": "default",
      "androidCollapsedTitle": "PackMind",
      "iosDisplayInForeground": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#a6c13c",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ],
      [
        "react-native-iap"
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "podfileProperties": {
              "use_modular_headers!": true
            }
          }
        }
      ],
      "@react-native-firebase/app"
    
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yutoliu.packmind",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesAppleSignIn": true
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserTrackingUsageDescription": "This allows PackMind to deliver timely packing reminders for your trips",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.yutoliu.packmind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "BILLING"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3883ba58-589a-45c5-a4d8-56db6d1d3d3a"
      }
    },
    "scheme": "com.yutoliu.packmind",
    "owner": "xiaohangliu"
  }
}
