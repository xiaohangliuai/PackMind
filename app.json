{
  "expo": {
    "name": "PackMind",
    "slug": "PackMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "entryPoint": "./index.ts",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#a6c13c",
      "androidMode": "default",
      "androidCollapsedTitle": "PackMind",
      "iosDisplayInForeground": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#a6c13c",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ],
      ["react-native-iap"]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yutoliu.packmind",
      "config": {
        "usesAppleSignIn": true
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserTrackingUsageDescription": "This allows PackMind to deliver timely packing reminders for your trips"
      }
    },
    "android": {
      "package": "com.yutoliu.packmind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "BILLING"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "scheme": "com.yutoliu.packmind"
  }
}
